body{
	background: #f2f2f2
}
.bordered{
	box-shadow: #1px 1px 1px #eee;
	border: 1px solid #ddd;
	border-radius: 2px;
	background: #fff
}

.page-header{
	padding: 10px;
	margin:0 10px;
}


#header{
	background: #1fb295;
	margin-bottom: 20px;
	box-shadow: 0 1px  3px #999;
	height: 66px
}

#header .main-nav a{
	color: #000;
	font-size: 16px;
	line-height: 1.2;
	padding-bottom: 5px;
	padding-top: 15px;
	padding-left: 2px;
	padding-right: 2px;
	margin: 7px 15px 0
}
#header .main-nav a:hover,#header .main-nav li.active a{
	color: #fff;
	background: #18b191;
	border-bottom: 2px solid #fff
};



#header .navbar-brand {
    font-size: 18px;
    height: 66px;
    line-height: 30px;
    padding: 15px;
}
#userLogin i{
	display: block;
	color: #333;
	font-size: 24px;
}
#userLogin>li>a{
	color: #fff;
	height:66px;
	padding:13px 15px;
	display: block;
}

#userLogin>li>a:hover,#userLogin>li>a:focus{
	color: #fff;
	background: #333
}

#userLogin>li.open>a{
	background: #333
}

#userLogin>li.open i{
	color: #fff
}

#userLogin>li>a:hover i,#userLogin>li>a:focus i{
	color: #fff;
}


#userLogin .share{
	font-size: 12px;
}

.img-circle{
	border: 2px solid #333
}

.share-menu {
    background-clip: padding-box;
    background-color: #fff;
    border: 1px solid rgba(0, 0, 0, 0.15);
    border-radius: 4px;
    box-shadow: 0 6px 12px rgba(0, 0, 0, 0.176);
    display: none;
    float: left;
    font-size: 14px;
    left: 0;
    list-style: outside none none;
    margin: 2px 0 0;
    min-width: 160px;
    padding: 10px;
    position: absolute;
    text-align: left;
    top: 100%;
    width: 189px;
    z-index: 1000;
}

/*footer*/
#footer{
	background: #333;
	color: #ccc;
	text-align: center;
	font-size: 12px;
	padding:5px 0;
}
/*公共右导航*/
.elevator {
  position: fixed;
  right: 15px;
  bottom: 10px;
  z-index: 1030;
}
@media(max-width:798px){
	.elevator {display:none}
}
.elevator a {
  display: block;
  position: relative;
  margin: 1px 0;
  outline: none;
  height: 52px;
  width: 52px;
  -webkit-transition: background-position 0.15s;
  -moz-transition: background-position 0.15s;
  transition: background-position 0.15s;
  background: url(../../images/elevator.png) no-repeat;
}
.elevator .elevator-diaocha {
  background-position: 0 -798px;
}
.elevator .elevator-diaocha:hover {
  background-position: 0 -984px;
}
.elevator .elevator-app-box {
  position: absolute;
  width: 172px;
  height: 194px;
  bottom: -10px;
  right: 46px;
  -webkit-transition: opacity 0.25s, transform .3s;
  -moz-transition: opacity 0.25s, transform .3s;
  transition: opacity 0.25s, transform .3s;
  opacity: 0;
  filter: alpha(opacity=0);
  max-width: 0;
  -webkit-transform: scale(0.01);
  -ms-transform: scale(0.01);
  transform: scale(0.01);
  -webkit-transform-origin: 100% 95%;
  -ms-transform-origin: 100% 95%;
  transform-origin: 100% 95%;
  background:url(../../images/elevator.png)no-repeat 0 -222px;
}
.elevator .elevator-app {
  background-position: 0 -550px;
}
.elevator .elevator-app:hover {
  background-position: 0 -612px;
}
.elevator .elevator-app:hover .elevator-app-box {
  display: block;
  visibility: visible;
  opacity: 1;
  filter: alpha(opacity=100);
  max-width: none;
  -webkit-transform: scale(1);
  -ms-transform: scale(1);
  transform: scale(1);
}
.elevator .elevator-weixin-box {
  position: absolute;
  /* display: none; */
  width: 172px;
  height: 212px;
  bottom: -10px;
  right: 46px;
  -webkit-transition: opacity 0.25s, transform .3s;
  -moz-transition: opacity 0.25s, transform .3s;
  transition: opacity 0.25s, transform .3s;
  opacity: 0;
  filter: alpha(opacity=0);
  max-width: 0;
  -webkit-transform: scale(0.01);
  -ms-transform: scale(0.01);
  transform: scale(0.01);
  -webkit-transform-origin: 100% 95%;
  -ms-transform-origin: 100% 95%;
  transform-origin: 100% 95%;
  background: url(../../images/elevator.png) no-repeat 0 0;
}
.elevator .elevator-weixin {
  background-position: 0 -798px;
}
.elevator .elevator-weixin:hover {
  background-position: 0 -860px;
}
.elevator .elevator-weixin:hover .elevator-weixin-box {
  /* max-width: none; */
  display: block;
  visibility: visible;
  opacity: 1;
  filter: alpha(opacity=100);
  max-width: none;
  -webkit-transform: scale(1);
  -ms-transform: scale(1);
  transform: scale(1);
}
.elevator .elevator-msg {
  background-position: 0 -426px;
}
.elevator .elevator-msg:hover {
  background-position: 0 -488px;
}
.elevator .elevator-top {
  background-position: 0 -674px;
}
.elevator .elevator-top:hover {
  background-position: 0 -736px;
}

/* 次导航 */
.navgation{ margin-top:-20px; background:#171e22; font-size:16px; color:#fff; }
.navgation a{ color:#fff; display:inline-block; margin-right:30px;}
.path{ padding:20px 20px; overflow:hidden; }
.path em{ display:inline-block; padding:0 5px; font-style:normal;}

.breadcrumb{
  background:none;
  font-size: 16px;
  padding: 0  15px 16px;
  margin: 0;
  color: #888
}
.breadcrumb a:link,.breadcrumb a:visited{
  color: #666
}
/* 底部 */
.footer{ height:35px; line-height:35px; text-align:center; color:#666; background:#232323; font-size:12px; margin-top:35px;}
/*分享*/
#bsWXBox{
  box-sizing: content-box !important;
}

.abcd-style{
  font-weight: bold;
  padding: 0 3px
}

#list .thumbnail h4{
    font-size: 16px;
    height: 48px;
    line-height: 1.5;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2; 
    -webkit-box-orient: vertical;
}